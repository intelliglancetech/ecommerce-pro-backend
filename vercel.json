{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "git": {
        "deploymentEnabled": true,
        "branches": ["master", "staging"]
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.js"
        }
    ]
}
